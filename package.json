{
  "name": "a10-widget",
  "version": "0.0.1",
  "private": true,
  "description": "A10 GUI Widget Library",
  "author": "Zuoping Li",
  "main": "./dist/index.js",
  "scripts": {
    "build": "better-npm-run build",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js dist/index.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/index.min.js",
    "clean": "rimraf dist lib",
    "example:A10Field": "node examples/A10Field/devServer.js",
    "prepublish": "npm run clean && npm run build",
    "lint": "eslint --fix -c .eslintrc client",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch"
  },
  "license": "MIT",
  "betterScripts": {
    "start-dev": {
      "command": "webpack-dev-server --config webpack/webpack.config.js --history-api-fallback --inline --hot --progress --port 3000",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.js ",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "mocha-webpack --colors  --webpack-config webpack.config.test.js \"client/**/__tests__/*.spec.js\"",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test:watch": {
      "command": "mocha-webpack --colors  --watch  --webpack-config webpack.config.test.js \"client/**/__tests__/*.spec.js\"",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.11",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.23.1",
    "es6-promise-promise": "^1.0.0",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash-webpack-plugin": "^0.10.6",
    "node-sass": "^3.5.0-beta.1",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.8",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "json-loader": "^0.5.4",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.1.0-beta",
    "webpack-hot-middleware": "^2.13.2"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "invariant": "^2.2.1",
    "jsoneditor": "^5.5.6",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "rc-slider": "^5.4.3",
    "react-addons-shallow-compare": "^15.4.2",
    "react-bootstrap": "^0.30.2",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-loading": "0.0.9",
    "react-select": "^1.0.0-rc.3",
    "react-tagsinput": "^3.14.0",
    "react-virtualized-select": "^2.3.0",
    "redux-immutable": "^3.0.8",
    "redux-thunk": "^2.1.0",
    "superagent": "^2.1.0",
    "tree-model": "^1.0.4"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.8"
  }
}
